{
  "name": "dbusana-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "D'Busana Fashion Dashboard Frontend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start-all": "node start-server.js",
    "debug": "node debug-server.js",
    "backend": "cd backend && npm run dev",
    "frontend": "vite",
    "troubleshoot": "node debug-server.js",
    "setup": "npm install && cd backend && npm install && cd ..",
    "reset": "rm -rf node_modules backend/node_modules && npm run setup",
    "health": "curl -s http://localhost:3000/health || echo 'Backend not running'",
    "test-api": "curl -s http://localhost:3000/api/products || echo 'API not accessible'",
    "test-duplicate-checker": "node scripts/testDuplicateCheckerAPI.js",
    "verify-duplicate-fix": "node scripts/verifyDuplicateCheckerFix.js",
    "update-deps": "node scripts/updateDependencies.js",
    "clean": "rimraf node_modules backend/node_modules",
    "clean-build": "rimraf dist backend/dist",
    "audit": "npm audit && cd backend && npm audit",
    "audit-fix": "npm audit fix && cd backend && npm audit fix",
    "outdated": "npm outdated && cd backend && npm outdated",
    "fix-duplicate-checker": "node scripts/verifyDuplicateCheckerFix.js",
    "test-duplicate-checker": "./test-duplicate-checker.sh || echo 'Run: chmod +x test-duplicate-checker.sh'",
    "verify-config": "node scripts/verifyDuplicateCheckerFix.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "lucide-react": "^0.263.1",
    "recharts": "^2.8.0",
    "react-hook-form": "^7.45.4",
    "motion": "^11.0.0",
    "sonner": "^2.0.3",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "class-variance-authority": "^0.7.0",
    "xlsx": "^0.18.5",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-slick": "^0.30.2",
    "react-responsive-masonry": "^2.1.7",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^4.0.0-alpha.24",
    "typescript": "^5.0.2",
    "vite": "^5.1.0",
    "@types/xlsx": "^0.0.36",
    "@types/react-slick": "^0.23.13",
    "rimraf": "^6.0.1",
    "glob": "^11.0.3"
  }
}