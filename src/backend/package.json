{
  "name": "dbusana-backend",
  "version": "1.0.0",
  "description": "D'Busana Fashion Dashboard Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "startup": "node startup.js",
    "dev": "nodemon src/server.js",
    "dev:safe": "node startup.js",
    "dev:cors": "node fix-cors-and-start.js",
    "fix-cors": "node simple-cors-fix.js",
    "emergency": "node emergency-restart.js",
    "emergency-cors": "node simple-cors-fix.js",
    "reset-limits": "node reset-rate-limits.js",
    "reset-rate-limits": "node reset-rate-limits.js",
    "emergency-restart": "node emergency-restart.js",
    "restart": "node emergency-restart.js",
    "fix-cors-dev": "node fix-cors-development-header.js",
    "build": "tsc",
    "check": "node src/utils/startupCheck.js",
    "test": "node test-server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node src/scripts/seed.js",
    "db:reset": "prisma migrate reset",
    "generate-templates": "node scripts/generate-templates.js",
    "templates": "node scripts/generate-templates.js",
    "fix-analytics": "node fix-prisma-analytics.js",
    "analytics-fix": "node fix-prisma-analytics.js",
    "diagnostic": "node diagnostic-startup.js",
    "diag": "node diagnostic-startup.js",
    "test-import": "node test-import-functionality.js",
    "test-imports": "node test-import-functionality.js",
    "generate-templates": "node generate-all-templates.js",
    "templates": "node generate-all-templates.js",
    "install-deps": "node install-dependencies.js",
    "setup-users": "node src/scripts/runUsersMigration.js",
    "user-migration": "node src/scripts/runUsersMigration.js",
    "verify-upgrades": "node verify-upgrades.js",
    "post-upgrade": "node verify-upgrades.js",
    "upgrade-complete": "node upgrade-dependencies.js && node fix-breaking-changes.js && node verify-upgrades.js",
    "fix-pg": "node fix-pg-dependency.js",
    "install-pg": "npm install pg@^8.12.0 @types/pg@^8.11.6",
    "quick-fix": "node quick-dependency-fix.js",
    "test-db": "node test-db-connection.js",
    "setup-db": "node setup-database.js",
    "product-hpp-migration": "node scripts/runProductHPPMigration.js",
    "test-product-hpp": "node scripts/testProductHPPAPI.js",
    "setup-product-hpp": "node scripts/setupProductHPPComplete.js",
    "update-deps": "npm update",
    "clean": "rimraf node_modules dist",
    "audit": "npm audit",
    "audit-fix": "npm audit fix",
    "outdated": "npm outdated",
    "test-isEqual": "node -e \"const {isEqual} = require('./src/utils/isEqualUtil'); console.log('Testing native isEqual:', isEqual({a:1}, {a:1}));\"",
    "update-deps-latest": "node scripts/updateDependenciesToLatest.js",
    "verify-deps": "node scripts/verifyDependencyUpdates.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^17.2.2",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "joi": "^18.0.1",
    "express-rate-limit": "^8.1.0",
    "compression": "^1.7.4",
    "moment": "^2.29.4",
    "uuid": "^11.1.0",
    "pg": "^8.12.0",
    "exceljs": "^4.4.0",
    "rimraf": "^4.0.0",
    "glob": "^9.0.0",
    "lru-cache": "^10.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.11.6",
    "typescript": "^5.6.2"
  },
  "keywords": ["fashion", "dashboard", "api", "nodejs", "express", "prisma"],
  "author": "D'Busana Team",
  "license": "MIT"
}